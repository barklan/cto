{
  "openapi": "3.1.0",
  "info": {
    "title": "CTO",
    "description": "https://redoc.ly/docs/cli/#.pathspets.yaml",
    "version": "1.0.1",
    "contact": {
      "name": "API Support",
      "email": "example@example.com"
    }
  },
  "servers": [
    {
      "url": "https://ctopanel.com/api"
    }
  ],
  "tags": [
    {
      "name": "auth",
      "description": "Authentication"
    },
    {
      "name": "log",
      "description": "Logs"
    },
    {
      "name": "users",
      "description": "Users"
    }
  ],
  "paths": {
    "/log/input": {
      "post": {
        "tags": [
          "log"
        ],
        "summary": "Fluentd input.",
        "security": [
          {
            "FluentdBasicAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "401": {
            "description": "Not Authorized"
          },
          "403": {
            "description": "Forbidden"
          }
        }
      }
    },
    "/log/exact": {
      "get": {
        "tags": [
          "log"
        ],
        "summary": "Exact log by key.",
        "parameters": [
          {
            "in": "query",
            "name": "key",
            "schema": {
              "type": "string"
            },
            "description": "Exact badger key."
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            },
            "description": "OK"
          },
          "401": {
            "description": "Not Authorized"
          },
          "403": {
            "description": "Forbidden"
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "TokenAuth": {
        "type": "apiKey",
        "name": "Authorization",
        "in": "header",
        "description": "Token-based authentication with required prefix \"Token\""
      },
      "FluentdBasicAuth": {
        "type": "http",
        "scheme": "basic",
        "description": "Fluentd basic auth. Username is project name, password is project secret.\n"
      }
    }
  }
}